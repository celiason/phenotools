#NEXUS

[new visual/eye characters]
[refs = Hancock 2010, Wood 1917, etc.]

BEGIN TAXA;
	DIMENSIONS NTAX=11;
	TAXLABELS
		Eudromia_elegans
		Rhynchotus_rufescens
		Struthio_camelus
		Rhea_americana
		Dromaius_novaehollandiae
		Anas_rubripes
		Chauna_torquata
		Gallus_domesticus
		Apteryx_mantelli
		Casuarius_unappendiculatus
		Anas_platyrhynchos
	;
END;

BEGIN CHARACTERS;
	DIMENSIONS NCHAR=7;
	FORMAT DATATYPE=STANDARD MISSING=? GAP=- SYMBOLS="012";
	CHARLABELS
		[1] 'Head-bobbing: [see Hancock 2010 dissertation]'
		[2] 'Retinal pattern:'
		[3] 'Fovea location'
		[4] 'Eye shine:'
		[5] 'Pecten, structural organization: [See Wood 1917, Meyer 1977, Kajikawa 1923, Vrabec 1958, Walls 1943]'
		[6] 'Pecten, shape/extension: [Wood 1917]'
		[7] 'Optic nerve fibers:'
		;
	STATELABELS
		1 absent 'sometimes present' present,
		2 afoveate unifoviate 'unifoveate with band-shaped area',
		3 nasal temporal 'linear fovea',
		4 absent present,
		5 conical vaned pleated [outgroups scored as 'pleated' based on knowledge that this is most common type in most birds, and congenerics analyzed by Wood 1917],
		6 'never extends into vitreous greater than length of widest segment (Wood 1917 type I)' 'does not project far into viterous, slopes away from visual axis (Wood 1917 type II)' 'projects deep into viterous almost to lens, follows strait or curved course along bulbus wall (Wood 1917 type III)',
		7 absent present,
	;
	MATRIX
		Eudromia_elegans 			210?101
		Rhynchotus_rufescens 		210?1?1
		Struthio_camelus 			11011??
		Rhea_americana 				(01)10?11?
		Dromaius_novaehollandiae 	1???2??
		Anas_rubripes 				0???2??
		Chauna_torquata 			0???2??
		Gallus_domesticus			200?2??
		Apteryx_mantelli			?10?02?
		Casuarius_unappendiculatus	210?200
		Anas_platyrhynchos			020?2??
		;
END;

BEGIN SETS;
	CHARPARTITION bodyparts=behavior:1, sensory:2-7;
END;
