idis(20), zlim=c(-1, 1))\ntitle(main=\"Trait correlation distance\")\nimage(max(mat2)-mat2[ord,ord], zlim=c(0, 6), col=viridis(20))\ntitle(main=\"Trait ontology distance\")\nboxplot(as.numeric(as.dist(abs(mat1))) ~ as.factor(as.numeric(as.dist(mat2))))\ndev.off()\n\nres.pe <- mantel(xdis=as.dist(abs(mat1)), ydis=as.dist(mat2), method=\"pearson\", permutations=999)\nres.pe\n\n# res.sp <- mantel(xdis=as.dist(mat1), ydis=as.dist(mat2), method=\"spearman\", permutations=999)\n# res.sp\n\nggplot(data=NULL, aes(y=as.numeric(as.dist(mat1)), x=as.factor(as.dist(mat2)))) +\n\t# geom_violin() +\n\t# geom_point(alpha=.1, pch=1) +\n\t# geom_jitter(width=.1, height=.1, alpha=.3, cex=1) +\n\tgeom_boxplot() +\n\t# geom_smooth(method=\"lm\") +\n\tlabs(y=\"Trait correlation\", x=\"Distance in trait ontology\")\n\n",
